/* FONTS */
h1.elementor-heading-title.elementor-size-large{
	font-size: 24px !important;
}
h3.elementor-cta__title.elementor-cta__content-item.elementor-content-item,
h3.elementor-heading-title {
	font-size: 20px !important;
}
h4.elementor-heading-title{ 
	font-size: 18px !important; 
}
.elementor-cta__description, .elementor-text-editor p{ 
	font-size: 16px !important; 
}
.elementor-widget-breadcrumbs {
    font-size: 1em !important;
}
.elementor-widget .elementor-widget-container h5{ 
	font-family: Open Sans, sans-serif;
    font-size: 20px;
    font-weight: 700;
    color: #001064;
}
.elementor-align-left .elementor-button-wrapper .elementor-button-text{
	text-align: left;
}

/* HEADER */
header{ display: none !important; }

/* MENÚ VERTICAL */
#vertical-menu .menu-item .elementor-item{ font-size: 15px !important; }
#vertical-menu .menu-item .menu-image-title-after{
	padding-left: 0 !important;
}
#vertical-menu .menu-item .menu-image-hover-wrapper{
	padding-left: 20px !important;
}
#vertical-menu .sub-menu.elementor-nav-menu--dropdown{
	min-width: 20em !important;
}
#vertical-menu .elementor-nav-menu--dropdown a{
	justify-content: initial !important;
}

/* NOTIFICACIONES */
.um-notification-b {
    display: none !important;
    background: none !important;
    box-shadow: none !important;
}
.um-notification-b .um-icon-ios-bell{ color: #444 !important; }
.um-notification-header .um-notification-right .um-notification-i-settings{
	display: none !important;
}
.um-notification.require_docs .um-notification-hide{
    display: none !important;
} 
.elementor-shortcode > .um-notification-b {
    display: block !important;
}
.um-notification-live-feed .um-notifications-none,
.um-notification-live-feed .um-notification.read {
    display: none !important;
}
.um-notification .um-notification-photo {
    display: none !important;
}
.um-notification span.b2 i {
    font-size: 30px !important;
    position: absolute !important;
    top: 10px !important;
    left: 20px !important;
}

/* BUSCADOR GENERAL */
.is-search-input { 
	-webkit-appearance: none;
	   -moz-appearance: none;
			appearance: none;
	border-radius: 5px 0 0 5px !important; 
}
.is-search-icon{
    border-radius: 0 5px 5px 0;
    border-left: none !important;
    background: #FFF !important;
}

/* GENERAL */
.elementor a:not([class]) {
	font-weight: 700;
	color: #0A93D5;
}
.elementor-slides-wrapper .elementor-background-overlay, 
.custom-card-over .elementor-cta__bg-overlay,
.custom-cta .elementor-cta__bg-overlay {
	opacity: .7;
	background: rgba(0,17,100,0);
	background: -moz-linear-gradient(top, rgba(0,17,100,0) 0%, rgba(0,17,100,0) 0%, rgba(0,17,100,0.67) 67%, rgba(0,17,100,1) 100%);
	background: -webkit-gradient(left top, left bottom, color-stop(0%, rgba(0,17,100,0)), color-stop(0%, rgba(0,17,100,0)), color-stop(67%, rgba(0,17,100,0.67)), color-stop(100%, rgba(0,17,100,1)));
	background: -webkit-linear-gradient(top, rgba(0,17,100,0) 0%, rgba(0,17,100,0) 0%, rgba(0,17,100,0.67) 67%, rgba(0,17,100,1) 100%);
	background: -o-linear-gradient(top, rgba(0,17,100,0) 0%, rgba(0,17,100,0) 0%, rgba(0,17,100,0.67) 67%, rgba(0,17,100,1) 100%);
	background: -ms-linear-gradient(top, rgba(0,17,100,0) 0%, rgba(0,17,100,0) 0%, rgba(0,17,100,0.67) 67%, rgba(0,17,100,1) 100%);
	background: linear-gradient(to bottom, rgba(0,17,100,0) 0%, rgba(0,17,100,0) 0%, rgba(0,17,100,0.67) 67%, rgba(0,17,100,1) 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#001164', endColorstr='#001164', GradientType=0 );
}
.custom-card-over .elementor-cta__title.elementor-cta__content-item.elementor-content-item{
	display: inline-block;
	clear: both;
	width: auto;
	position: relative;
}
.custom-card-over.center .elementor-cta__title.elementor-cta__content-item.elementor-content-item {
	margin-left: auto;
	margin-right: auto;
}
.custom-card-over .elementor-cta__title.elementor-cta__content-item.elementor-content-item:after {
    content: '';
    width: 15px;
    height: 15px;
    position: absolute;
    top: -11px;
    right: -22px;
    background: url(/wp-content/uploads/2020/06/points.png) no-repeat center /contain;
    z-index: 999;
}
.custom-rounded-cards .elementor-bg {
	border-radius: 10px;
}
.dialog-type-lightbox{ background-color: rgba(0,0,0,.6) !important; }

/* CURRENT USER */
.custom-box-current-user{
    margin: 0px 0px 30px 0px;
    padding: 0px 10px 0px 20px;
}
.elementor-author-box__name {
    color: #595959 !important;
    font-size: 20px !important;
    margin-bottom: 2px !important;
}
.elementor-author-box__button {
    background-color: #FFFFFF !important;
    color: #BF0086 !important;
    border-color: #BF0086 !important;
    font-size: 15px !important;
    font-weight: 600 !important;
    border-width: 0px !important;
    padding: 0px 0px 0px 0px !important;
}

/* DIRECTORIO */
.um-directory .um-member-directory-header .um-member-directory-header-row.um-member-directory-search-row {
    justify-content: flex-start !important;
}
.um-directory .um-member-directory-header .um-member-directory-header-row .um-member-directory-search-line .um-do-search {
    padding: 12.5px 10px !important;
    background: #bf0086 !important;
}
#directory-search-form .forminator-response-message{
	display: none !important;
}
#directory-search-form .forminator-dropdown-list{
	position: absolute;
	top: 100%;
	z-index: 999;
}
#directory-active-form {
    margin-bottom: 25px;
}
#directory-active-form .col-form {
    display: inline;
    float: left;
    margin-right: 1%;
    margin-bottom: 1%;
}
#directory-active-form .col-form.col-search-directory {
    width: 47%;
}
#directory-active-form .col-form.col-office-directory {
    width: 35%;
}
#directory-active-form .col-form.col-submit-directory {
    width: 10%;
}
#directory-active-form .input-search-directory,
#directory-active-form .input-office-directory{
    width: 100% !important;
}
#directory-active-form .input-office-directory {
    height: 42px;
}
#directory-active-form .input-submit-directory {
    background-color: #17A8E3 !important;
    color: #FFF !important;
    border: none !important;
}
#directory-active .row-directory-active{ 
	display: flex;
	justify-content: flex-start;
	align-items: stretch;
	align-content: flex-start;
	flex-wrap: wrap;
}
#directory-active .col-directory-active{ 
	padding: 10px;
	flex-basis: 20%;
}
#directory-active .directory-card{
	overflow: hidden;
	box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.3);
	border-radius: 20px;
}
#directory-active .directory-image,
#directory-active .directory-image-bg{
	margin: 0;
	padding: 0;
	height: 150px;
}
#directory-active .directory-image{ overflow: hidden; }
#directory-active .directory-image-bg{
	background-repeat: no-repeat;
	background-size: cover; 
	background-position: center;
	-webkit-transition: all .4s ease-out; 
	   -moz-transition: all .4s ease-out; 
	    -ms-transition: all .4s ease-out; 
			transition: all .4s ease-out; 
}
#directory-active .directory-image:hover,
#directory-active .directory-content h4:hover{ cursor: pointer; }
#directory-active .directory-image:hover .directory-image-bg{
	-webkit-transform: scale(1.1);
	   -moz-transform: scale(1.1);
		-ms-transform: scale(1.1);
			transform: scale(1.1);
}
#directory-active .directory-content{
	position: relative;
	min-height: 180px;
	padding: 60px 20px 20px;
	background-color: #FFF;
	text-align: center;
}
#directory-active .directory-content h4{
	margin-bottom: 10px;
    font-size: 18px;
    font-weight: 700;
    color: #001064;
}
#directory-active .directory-icon-wrapper{
	position: absolute;
	top: 0;
	left: 50%;
	margin: -30px;
	z-index: 99;
}
#directory-active .directory-icon{
	display: block;
	width: 60px;
	height: 60px;
	line-height: 60px;
	font-size: 30px;
	text-align: center;
	background-color: #BF0086;
    color: #FFFFFF;
    border-radius: 30px;
}
.directory-pagination{ margin-top: 30px; }
.directory-pagination .ul-pagination{ list-style-type: none; padding: 0; }
.directory-pagination .ul-pagination a{ display: inline-block; padding: 10px; }
.directory-pagination .ul-pagination a.active{ color: #001064 !important; }
.directory-pagination .ul-pagination a.disable{ pointer-events: none; opacity: .7; }
.ui-autocomplete.ui-menu{ max-height: 50vh !important; overflow-y: scroll; padding: 20px; }
.ui-autocomplete.ui-menu .ui-menu-item {
    padding: 0 0 10px !important;
    font-size: 15px !important;
}
.ui-autocomplete.ui-menu .ui-menu-item strong{
    background-color: #FFFFB9 !important;
}
.ldap_error{ display: none !important; }

/* APPS */
.col-form.col-app-categories {
    max-width: 600px;
}
.content-app .elementor-icon {
	background-color: transparent !important;
    background-image: linear-gradient(180deg, #4EC9D1 0%, #0A93D5 100%) !important;
}
#applications-list {
    margin-top: 10px;
    margin-bottom: 10px;
}
.row-apps {
    display: flex;
    justify-content: flex-start;
    align-items: stretch;
    align-content: flex-start;
	flex-wrap: wrap;
}
.row-apps-home {
    display: flex;
    justify-content: center;
    align-items: stretch;
    align-content: center;
	flex-wrap: wrap;
}
.col-apps {
	display: flex;
    align-items: stretch;
    flex-basis: 33.3%;
    padding: 10px;
}
.col-apps.hidden-app{ display: none !important; }
.col-apps-home {
	display: flex;
    align-items: stretch;
    flex-basis: 50%;
    padding: 10px;
}
.card-app-wrapper {
	width: 100%;
	position: relative;
    padding: 40px 25px 60px 20px;
    box-shadow: 0 0 10px 0 rgba(0,0,0,.3);
    border-radius: 10px;
    background-color: #FFF;
}
.card-app-wrapper-home {
	width: 100%;
	position: relative;
    padding: 20px;
    box-shadow: 0 0 10px 0 rgba(0,0,0,.3);
    border-radius: 10px;
    background-color: transparent !important;
    background-image: linear-gradient(180deg, #4EC9D1 0%, #0A93D5 100%) !important;
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    align-content: center;
    justify-content: center;
}
.card-app-wrapper-home, .card-app-wrapper-home a{ text-align: center; }
.card-app-wrapper-home .icon-box-app-home {
    flex-basis: 100%;
    text-align: center;
    font-size: 30px;
    color: #FFF !important;
}
.card-app-wrapper-home .img-app-box-home {
    width: auto;
    height: auto;
    max-height: 25px;
    margin: auto;
}
.card-app-wrapper .category-app{
    position: absolute;
    top: 10px;
    left: -15px;
    padding: 5px 10px;
    background-color: #001064;
    font-size: 13px;
    font-weight: 700;
    color: #FFF;
    text-transform: uppercase;
    z-index: 9;
}
.card-app-wrapper .category-app::before{
    display: block;
    width: 20px;
    height: 0px;
    position: absolute;
    bottom: -10px;
    left: -5px;
    content: "";
    border-bottom: 10px solid transparent;
    border-right: 14px solid rgb(0, 80, 116);
}
.card-app-wrapper-home h5 {
    flex-basis: 100%;
    font-size: 14px !important;
    text-align: center;
    color: #FFF !important;
}
a.btn-hide-app {
    position: absolute;
    top: 20px;
    right: 20px;
    font-size: 18px;
}
.row-inner-app {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    align-content: flex-start;
    min-height: 140px;
}
.col-inner-app:first-child {
    flex-basis: 30%;
    padding: 3px;
    text-align: center;
}
.col-inner-app:last-child {
    flex-basis: 70%;
    padding: 10px;
}
.icon-box-app {
    width: 80px;
    height: 80px;
    line-height: 80px;
    background-color: transparent !important;
    background-image: linear-gradient(180deg, #4EC9D1 0%, #0A93D5 100%) !important;
    border-radius: 40px;
    font-size: 25px;
    color: #FFF;
}
.img-app-box {
    width: 80px;
    height: auto;
    border: none;
    vertical-align: middle;
}
.col-inner-app h5 {
    font-size: 18px !important;
    margin: 0 0 5px !important;
}
.row-buttons-app {
    position: absolute;
    left: 20px;
    bottom: 20px;
}
a.btn-app {
    font-size: 13px !important;
    font-weight: 700;
    padding: 6px 8px;
    margin-right: 4px;
    border-radius: 3px 3px 3px 3px;
}
a.btn-app.btn-link-app {
    background-color: #BF0086;
    color: #FFFFFF;
}
a.btn-app.btn-link-manual,
a.btn-app.btn-popup-help {
    color: #001064;
    background-color: #EFEFF1;
}
a.btn-link-hide-apps{
	background-color: #001064;
    color: #FFFFFF !important;
}
.link_hide_apps{ margin-top: 30px; padding: 10px; }
.list-manuals {
    list-style-type: none;
    text-align: center;
}
.list-manuals li {
    padding: 5px 0;
    border-bottom: dotted 1px #CCC;
}
.list-manuals li a {
    font-weight: 700;
}

/* UTILIDADES */
#utilities-list {
    margin-bottom: 10px;
}
.row-utilities {
	display: flex;
    justify-content: flex-start;
    align-items: stretch;
    align-content: center;
    flex-wrap: wrap;
}
.col-utilities {
	display: flex;
	align-items: stretch;
    padding: 20px 30px;
}
.col-utilities.hidden-utility { display: none !important; }
.card-utility-wrapper {
	position: relative;
    width: 160px;
    height: 160px;
    padding: 20px;
    border: solid 2px #001064;
    border-radius: 50%;
}
.icon-box-utility:hover,
.title-utility:hover { cursor: pointer; }
a.btn-hide-utility {
    position: absolute;
    top: 7px;
    right: 5px;
    width: 35px;
    height: 35px;
    line-height: 35px;
    text-align: center;
    background: #BF0086;
    border-radius: 50%;
    color: #FFF !important;
}
.icon-box-utility {
    font-size: 35px;
    color: #001064;
    text-align: center;
}
.card-utility-wrapper h5 {
    font-size: 16px !important;
    text-align: center;
}
.link_hide_utilities{ margin-top: 30px; padding: 10px; }
.menu-item-utility a i{
    font-size: 16px;
    margin-right: 10px;
}
.menu-item-utility a span {
    font-family: 'Open Sans', sans-serif;
    font-size: 13px;
    font-weight: 700;
    height: auto;
    hyphens: manual;
    line-height: 20px;
    padding-left: 0px;
    text-align: left;
    text-size-adjust: 100%;
}

/* POPUPS */
.overlay-popups {
    display: none;
    position: fixed;
    width: 100vw;
    height: 100vh;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0, .8);
    z-index: 99999;
}
.overlay-popups.active{ display: block; }
.popup {
    width: 90%;
    max-width: 1000px;
    padding: 20px 40px;
    position: relative;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    background: #FFF;
}
.close-popup {
    position: absolute;
    top: 10px;
    right: 20px;
    font-size: 25px;
}
.close-popup:hover {
    cursor: pointer;
}
/* POP UP DIRECTOY */
.popup-directory .row-popup {
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.popup-directory .row-popup .col-popup {
    flex-basis: 50%;
}
.col-popup.bg-image-directory {
    flex-basis: 300px !important;
	background-image:url(/wp-content/uploads/2020/08/default-user-img.jpg);
	background-position: center;
	background-repeat: no-repeat;
	background-size: cover;
    height: 300px;
}
.col-popup.content-directory {
    flex-basis: calc(100% - 300px) !important;
    padding: 30px;
}
.col-popup.content-directory .title-directory-employee {
	margin-bottom: 30px;
	font-size: 20px;
    font-weight: 700;
    font-family: 'Open Sans';
    color: #001064;
}
.content-directory .row-inner-popup {
    display: flex;
    justify-content: flex-start;
    align-items: start;
    margin-bottom: 10px;
}
.content-directory .row-inner-popup .col-inner-popup {
    flex-basis: 50%;
    padding: 4px;
}
.content-directory .row-inner-popup .col-inner-popup i{
    display: inline;
    float: left;
    margin-top: 4px;
	margin-right: 10px;
	font-size: 20px;
	color: #BF0086;
}
.content-directory .row-inner-popup .col-inner-popup span{
    display: inline;
    float: left;
    width: calc(100% - 40px);
	font-size: 15px;
}
.content-directory .row-inner-popup .col-inner-popup span a{
    color: #333 !important;
}
.content-directory .row-inner-popup .col-inner-popup span small{
    font-size: 85% !important;
}

/* POP UP APPS */
.popup-apps.popup {
    padding: 40px !important;
    text-align: center;
}
.title-popup-apps {
    font-family: 'Open Sans', sans-serif;
    font-size: 18px;
    font-weight: 700;
}
.content-popup-apps {
    width: 100%;
    max-width: 400px;
    margin: 0 auto 30px;
}
.popup-apps .btn-link-app.hide{ 
	display: none; 
}

/* POP UP APPS */
.popup-utilities.popup {
    padding: 40px !important;
    text-align: center;
}
.title-popup-utilities {
    font-family: 'Open Sans', sans-serif;
    font-size: 18px;
    font-weight: 700;
}
.content-popup-utilities {
    width: 100%;
    max-width: 400px;
    margin: 0 auto 30px;
}
.link-manual-popup-utilities a {
    font-weight: 700;
    text-decoration: underline;
    font-size: 15px;
}
.popup-utilities .btn-link-app.hide{ 
	display: none; 
}

/* PRL */
.accordion-list .elementor-tab-content ul{
	list-style-type: none !important;
	margin: 10px 0 0 0;
	padding: 0;
}
.accordion-list .elementor-tab-content ul li{
	list-style-type: none !important;
	padding: 10px 0;
	border-bottom: solid 1px #CCC;
}
.accordion-list .elementor-tab-content,
.accordion-list .elementor-accordion-item{
	border-top: none !important;
}

.custom-card-z .elementor-widget-container,
.custom-card-z .elementor-cta{
	overflow: visible !important;
}
.custom-card-z .elementor-cta__content{
	position: absolute;
    top: 80%;
    right: -10px;
    width: 70%;
    z-index: 9;
    box-shadow: 0px 0px 10px 0px rgba(0, 0, 0, 0.3);
    min-height: 200px;
}
.custom-card-z .elementor-cta__button {
	padding: 0 !important;
}

/* MENÚ DEL DÍA */
.todays-menu-title{ 
	margin-bottom: 30px !important;
    font-family: 'Open Sans', sans-serif;
    font-weight: 700;
    color: #001064;
}
.dishes-row-menu {
    display: flex;
    flex-wrap: wrap;
    justify-content: flex-start;
    align-items: start;
}
.dishes-title{ margin: 10px 0 !important; padding-bottom: 10px !important; border-bottom: dashed 1px #DDD; font-size: 14px; }
.dishes-col-menu {
    flex-basis: 50%;
}
.dishes-list-menu{ list-style-type: none; padding-left: 10px; margin-bottom: 20px !important; }
.dishes-list-menu li{ margin-bottom: 5px; font-size: 14px; }
.dishes-list-menu li i { margin-right: 10px; color: #001064; }

/* REPOSITORIO */
#repository .panel.panel-default.card.category-panel.wpdm-shortcode-toolbar,
#repository .panel-footer.card-footer {
    border: none !important;
}
#repository .panel-body.card-body {
    display: none !important;
}
#repository .panel-footer.card-footer{
	padding: 0 !important;
	background-color: #FFF;
}
#repository .panel-footer.card-footer .col-lg-6,
#repository .panel-footer.card-footer .col-lg-2{
	margin-bottom: 10px;
}
#repository .wpdm-custom-select{ padding-right: 30px !important; }
#repository .wpdm-download-link.btn.btn-primary {
	margin-top: 6px;
}
#repository .wpdm-link-template {
    padding: 20px 20px 20px 0 !important;
}
#repository .wpdm-link-template a.wpdm-download-link {
	font-family: "Open Sans", Sans-serif !important;
	font-weight: 700;
}
#repository .card-body {
    padding: 5px 1.25rem !important;
}
#repository .link-template-default .package-title > a {
	font-family: "Open Sans", Sans-serif !important;
	font-size: 14px !important;
}
#repository .wpdm_icon {
    width: 40px !important;
    min-width: unset !important;
}
#repository .text-muted.text-small {
    font-size: 10px !important;
}
#repository-tree .elementor-nav-menu > li {
	border: solid 1px #CCC;
	border-radius: 5px;
}
#repository-tree .sub-menu.elementor-nav-menu--dropdown{
	min-width: 20em !important;
}
.tree-repository {
    list-style-type: none;
    list-style-position: inside;
}
.main-menu-repository {
    padding: 0 !important;
}
.tree-repository li {
    padding: 8px 10px;
    -webkit-transition: all .4s ease-out;
       -moz-transition: all .4s ease-out;
        -ms-transition: all .4s ease-out;
            transition: all .4s ease-out;
}
.menu-item-repository {
    border: solid 1px #DDD;
    border-radius: 5px;
    margin-bottom: 5px;
}
.tree-repository li > a {
    float: left;
    text-decoration: none;
    color: #595959 !important;
    font-size: 15px;
    font-weight: 400;
}
.tree-repository .sub-menu li > a {
    font-size: 14px !important;
}
.tree-repository li.has-parent > a{
    max-width: 80%;
}
.tree-repository .submenu-handler {
    float: right;
}
.tree-repository .clearfix{ clear: both; }
.tree-repository li > a > i {
    margin-right: 5px;
}
.tree-repository li:hover{ background: #0A93D5 !important; cursor: pointer; }
.tree-repository li:hover *{ color: #FFF !important; }
.sub-menu.sub-menu-repository {
    display: none;
}

/* NOTIFICACIONES LECTURA OBLIGATORIA */
#notification-status-form {
    margin-bottom: 15px;
}
#notification-status-form .col-form {
    display: inline;
    float: left;
    margin-right: 1%;
    margin-bottom: 1%;
}
#notification-status-form .col-form.status-notification{
    width: 47%;
}
#notification-status-form .col-form.col-submit-notification {
    width: 10%;
}
#notification-status-form .input-status-notification{
    width: 100% !important;
    height: 42px;
}
#notification-status-form .input-submit-notification {
    background-color: #17A8E3 !important;
    color: #FFF !important;
    border: none !important;
}
.notification-doc {
    margin: 0;
    padding: 20px 20px 30px 0;
    border-bottom: solid 1px #DDD;
}
.title-notification-doc {
    margin: 0 0 2px;
    font-size: 16px;
    font-weight: 700;
    color: #444;
}
.title-notification-doc:hover {
    color: #0A93D5;
    text-decoration: underline;
}
.link-notification-doc {
    font-size: 16px;
    font-weight: 700;
    color: #444;
    text-decoration: underline;
    margin-right: 5px;
}
.link-notification-doc:hover {
    color: #0A93D5;
    text-decoration: underline;
}
.date-notification-doc {
    margin: 0 0 8px;
    font-size: 14px;
    color: #444;
}
.description-notification-doc {
    margin: 0 0 10px;
    font-size: 15px;
    color: #555;
}
.description-notification-doc span {
    margin: 0 2px;
    padding: 5px 10px;
    background: none;
    color: #444;
    border-radius: 20px;
}
.description-notification-doc .bubble-status { color: #FFF; }
.notification-doc.pending .bubble-status{ background-color: #cf000f; }
.notification-doc.confirm .bubble-status{ background-color: #2ecc71; }
.notification-doc.expires .bubble-status{ background-color: #bdc3c7; }
.description-notification-doc strong i {
    margin-right: 5px;
}

@media (min-width: 992px) {
	#repository .panel-footer.card-footer .col-lg-6 {
	    -ms-flex: 0 0 100% !important;
	    flex: 0 0 100% !important;
	    max-width: 100% !important;
	}
	#repository .panel-footer.card-footer .col-lg-2 {
	    -ms-flex: 0 0 33% !important;
	    flex: 0 0 33% !important;
	    max-width: 33% !important;
	}
}

/* PROFILE */
.um-profile .um-profile-edit{ display: none !important; }

/* CUSTOM POST */
#custom-posts-list{ margin: 30px 0; }
.row-custom-posts {
    display: flex;
    justify-content: flex-start;
    align-items: stretch;
    align-content: flex-start;
    flex-wrap: wrap;
}
.col-custom-posts {
    display: flex;
    align-items: stretch;
    flex-basis: 33.3%;
    padding: 10px;
}
.card-custom-post {
    width: 100%;
    position: relative;
    padding: 20px 25px 20px 20px;
    box-shadow: 0 0 10px 0 rgba(0,0,0,.3);
    border-radius: 10px;
    background-color: #FFF;
}
.card-custom-post .ribbon-post{
    position: absolute;
    top: 30px;
    left: -15px;
    padding: 5px 10px;
    background-color: #001064;
    font-size: 13px;
    font-weight: 700;
    color: #FFF;
    text-transform: uppercase;
    z-index: 9;
}
.card-custom-post .ribbon-post::before{
	display: block;
    width: 20px;
    height: 0px;
    position: absolute;
    bottom: -10px;
    left: -5px;
    content: "";
    border-bottom: 10px solid transparent;
    border-right: 14px solid rgb(0, 80, 116);
}
.card-custom-post .bg-image-wrapper{
    position: relative;
    height: 200px;
    margin-bottom: 30px;
    overflow: hidden;
}
.card-custom-post .bg-image-post{
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background-size: cover;
    background-position: center;
    background-repeat: no-repeat;
    transition: all .4s ease-out;
}
.card-custom-post .bg-image-wrapper:hover .bg-image-post{
    -webkit-transform: scale(1.1);
       -moz-transform: scale(1.1);
            transform: scale(1.1);
}
.card-custom-post .title-custom-post {
    margin-bottom: 10px;
    font-size: 20px;
    font-weight: 700;
    color: #001064;
}
.card-custom-post .link-post{
    display: inline-block;
    margin: 10px 0 0;
    padding: 10px 20px;
    font-size: 14px;
    font-weight: 700;
    color: #FFFFFF !important;
    text-decoration: none;
    border-color: #BF0086;
    background-color: #BF0086;
}
.wrapper-vacancies .bg-image-wrapper{ height: 300px !important; }
.wrapper-vacancies .bg-image-wrapper .bg-image-post{ background-position: center bottom !important; }

